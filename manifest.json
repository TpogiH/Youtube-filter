{
    "manifest_version": 2,
    "name": "Youtube filter",
    "description": "Добавляет фильтр на странице со списком видео.",
    "version": "1.0.3",
    "icons": {
        "16": "images/icon_16.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "browser_action": {
        "default_title": "Youtube filter",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": [
            "https://www.youtube.com/*"
        ],
        "js": ["js/jquery-3.5.1.min.js", "js/content.js"],
        "css": ["css/append.css"]
    }]
}